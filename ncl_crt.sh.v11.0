#----------------------------------
# read parameters from command line
#----------------------------------

k=1
for ((i=1; i<=$pram_num; i=i+1))
do
  # read every parameters
  pram[i]=$1
  shift

  # read position of indexes
  case "${pram[$i]}" in
    [-][f] | [-][f][i][l][e] )
      idx_pos[k]=$i
      k=$(($k+1))
      ;;

    [-][v] | [-][v][a][r] )
      idx_pos[k]=$i
      k=$(($k+1))
      ;;

    [-][t] | [-][t][y][p][e] )
      idx_pos[k]=$i
